def fibonacci(n):
    fib_series = []
    a = 0
    b = 1
    for i in range(n):
        fib_series.append(a)
        a, b = b, a + b
    return fib_series
    
n = int(input("Enter the number : "))
print("Non Recursive",fibonacci(n))

def fibonacci_recursive(n):
    if n <= 0:
        return []
    elif n == 1:
        return [0]
    elif n == 2:
        return [0, 1]
    else:
        fib_series = fibonacci_recursive(n - 1)
        fib_series.append(fib_series[-1] + fib_series[-2]) 
    return fib_series
    
print("Recursive", fibonacci_recursive(n))
